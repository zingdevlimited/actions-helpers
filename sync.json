{
  "$schema": "https://raw.githubusercontent.com/zingdevlimited/actions-helpers/v3/.schemas/update-sync.json",
  "documents": [
    {
      "uniqueName": "locale-config",
      "defaultData": {
        "locales": [
          "en-gb",
          "en-us"
        ],
        "default": "en-gb"
      },
      "aclPermissions": ["WRITE_ONLY"]
    }
  ],
  "lists": [
    {
      "uniqueName": "teams",
      "aclPermissions": [
        "READ_ONLY"
      ]
    }
  ],
  "maps": [
    {
      "uniqueName": "opening-hours",
      "defaultItems": [
        {
          "key": "sales",
          "data": {
            "start": "08:00",
            "end": "17:00"
          }
        }
      ]
    }
  ]
}